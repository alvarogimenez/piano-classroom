<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="60.0" prefWidth="600.0" style="-fx-border-style: solid; -fx-border-width: 1px; -fx-border-color: #ccc;" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1">
   <left>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="label_channel_name" text="Channel" />
            <Label fx:id="label_gain" alignment="CENTER" maxWidth="100.0" prefWidth="100.0" text="- 10.5" textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
         </children>
      </VBox>
   </left>
   <center>
      <BorderPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <left>
            <VBox alignment="CENTER" prefHeight="60.0" prefWidth="70.0" spacing="4.0" BorderPane.alignment="CENTER">
               <children>
                  <ToggleButton maxHeight="22.0" minHeight="22.0" mnemonicParsing="false" prefHeight="22.0" prefWidth="45.0" styleClass="solo-toggle-button" text="Solo">
                     <font>
                        <Font size="10.0" />
                     </font></ToggleButton>
                  <ToggleButton maxHeight="22.0" minHeight="22.0" mnemonicParsing="false" prefHeight="22.0" prefWidth="45.0" styleClass="mute-toggle-button" text="Mute">
                     <font>
                        <Font size="10.0" />
                     </font></ToggleButton>
               </children>
            </VBox>
         </left>
         <padding>
            <Insets right="5.0" />
         </padding>
         <center>
            <BorderPane fx:id="bpane_mix_channel" maxHeight="50.0" prefHeight="50.0" BorderPane.alignment="CENTER" />
         </center>
      </BorderPane>
   </center>
</BorderPane>
